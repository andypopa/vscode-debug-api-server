{
	"name": "vscode-debug-api-server",
	"displayName": "vscode-debug-api-server",
	"description": "Starts a server which you can use to start, stop, restart VSCode debugging sessions.",
	"version": "0.0.4",
	"publisher": "andypopa",
	"repository": {
		"type": "git",
		"url": "https://github.com/andypopa/vscode-debug-api-server.git"
	},
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/cors": "^2.8.4",
		"@types/express": "^4.16.1",
		"@types/lodash": "^4.14.121",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/socket.io": "^2.1.2",
		"@types/socket.io-client": "^1.4.32",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"body-parser": "^1.18.3",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"lodash": "^4.17.11",
		"socket.io": "^2.2.0",
		"socket.io-client": "^2.2.0"
	}
}
